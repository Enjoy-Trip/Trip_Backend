<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.trip.news.mapper.NewsMapper">

	<resultMap type="NewsDto" id="newsDto">
		<result column="news_no" property="news_no"/>
		<result column="news_title" property="news_title"/>
		<result column="news_content" property="news_content"/>
		<result column="news_link" property="news_link"/>
		<result column="news_image" property="news_image"/>
		<result column="news_time" property="news_time"/>
	</resultMap>
	
	<select id="newsList" resultMap="newsDto">
		select *
		from news
	</select>
</mapper>